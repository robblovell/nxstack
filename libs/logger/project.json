{
  "root": "libs/logger",
  "sourceRoot": "libs/logger/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/logger/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/logger"
      ],
      "options": {
        "jestConfig": "libs/logger/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
